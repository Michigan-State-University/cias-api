{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "minItems": 1,
      "maxItems": 1,
      "items": [
        {
          "type": "object",
          "properties": {
            "payload": {
              "type": "object",
              "properties": {
                "days_count": {
                  "type": "string"
                },
                "start_date": {
                  "type": "string"
                },
                "end_date": {
                  "type": "string"
                },
                "choose_date_range": {
                  "type": "boolean"
                }
              },
              "required": [
                "days_count",
                "choose_date_range"
              ],
              "additionalProperties": false
            }
          },
          "required": [
            "payload"
          ],
          "additionalProperties": false
        }
      ]
    }
  },
  "required": [
    "data"
  ],
  "additionalProperties": false
}

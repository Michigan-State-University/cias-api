{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "minItems": 1,
      "maxItems": 1,
      "items": [
        {
          "type": "object",
          "properties": {
            "payload": {
              "type": "object",
              "properties": {
                "end_value": {
                  "type": "string"
                },
                "start_value": {
                  "type": "string"
                },
                "original_text": {
                  "end_value" : {
                    "type": "string"
                  },
                  "start_value": {
                    "type": "string"
                  }
                }
              },
              "required": [
                "end_value",
                "start_value"
              ],
              "additionalProperties": false
            }
          },
          "required": [
            "payload"
          ],
          "additionalProperties": false
        }
      ]
    },
    "variable": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "data",
    "variable"
  ],
  "additionalProperties": false
}
